<p id="notice"><%= notice %></p>

<h1>Visa Details</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @visa_details.each do |visa_detail| %>
      <tr>
        <td><%= visa_detail.try(:user).try(:family_name) %></td>
        <td><%= visa_detail.try(:user).try(:gender) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visa Detail', new_visa_detail_path %>
